datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}
model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String  @unique
  password String
}
model Booking {
  id          String   @id @default(uuid())
  apartmentId String
  userId      String
  startDate   DateTime
  endDate     DateTime
  createdAt   DateTime @default(now())
}
